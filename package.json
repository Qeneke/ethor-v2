{
  "name": "ethor",
  "description": "this is ethor",
  "version": "2.0.0",
  "main": "build/index.js",
  "scripts": {
    "_dev:npm-outdated": "npm outdated --long",
    "_dev:npm-update": "npm update",
    "_dev:cost-of-modules:prod": "cost-of-modules --no-install",
    "_dev:cost-of-modules:dev": "cost-of-modules --no-install --include-dev",
    "_dev:apollo": "apollo",
    "analyze": "cross-env BUNDLE_ANALYZE=both npm run build:www",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server npm run build:www",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser npm run build:www",
    "build": "concurrently \"npm:build:*\"",
    "build:server": "npx typescript --project tsconfig.json",
    "build:www": "cross-env SPAWN_CODE=nextQbuild@./www/nextjs/%sQ_WWW_NEXTJS node script.js",
    "clean:cache": "concurrently \"npm:clean:cache:*\"",
    "clean:cache:build": "rimraf -rf ./build",
    "clean:cache:coverage": "rimraf -rf ./coverage",
    "clean:cache:coverage:www": "cross-env SPAWN_CODE=rimrafQ-rf@./www/nextjs/%s/coverageQ_WWW_NEXTJS node script.js",
    "clean:cache:node_modules": "rimraf -rf ./node_modules/.cache",
    "clean:cache:www:.next": "cross-env SPAWN_CODE=rimrafQ-rf@./www/nextjs/%s/.nextQ_WWW_NEXTJS node script.js",
    "clean:cache:bundles": "cross-env SPAWN_CODE=rimrafQ-rf@./www/nextjs/%s/bundlesQ_WWW_NEXTJS node script.js",
    "dev:api": "cross-env DEBUG=*,-express:*,-finalhandler,-body-parser:*,-selectn NODE_ENV=development _API=true ts-node --log-error --project tsconfig.json --files src/nodejs/index.ts",
    "dev:www": "cross-env DEBUG=*,-express:*,-finalhandler,-body-parser:*,-babel*,-send,-compression NODE_ENV=development _WEB=true ts-node --log-error --project tsconfig.json --files src/nodejs/index.ts",
    "lint": "eslint --ext .ts,.js ./ && cross-env SPAWN_CODE=eslintQ--ext@.ts,.tsx,.js,.jsx@./www/nextjs/%sQ_WWW_NEXTJS node script.js",
    "lint:debug": "eslint --debug --ext .ts,.js ./",
    "start": "cross-env NODE_ENV=production _API=true _WEB=true node build/src/nodejs/index.js",
    "start:api": "cross-env NODE_ENV=production _API=true node build/src/nodejs/index.js",
    "start:www": "cross-env NODE_ENV=production _WEB=true node build/src/nodejs/index.js",
    "storybook-start": "cross-env SPAWN_CODE_2=start-storybookQ-p@%d@-c@./www/nextjs/%s/.storybookQ_WWW_NEXTJS node script.js",
    "test": "cross-env NODE_ENV=test jest",
    "test:debug": "cross-env NODE_ENV=test jest --debug",
    "test:updateSnapshot": "cross-env NODE_ENV=test jest --updateSnapshot",
    "test:updateSnapshot:debug": "cross-env NODE_ENV=test jest --updateSnapshot --debug",
    "test:watch": "cross-env NODE_ENV=test jest --watchAll",
    "test:watch:debug": "cross-env NODE_ENV=test jest --watchAll --debug"
  },
  "dependencies": {
    "@apollo/federation": "^0.8.0",
    "@apollo/gateway": "^0.8.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/debug": "^4.1.5",
    "@types/dotenv": "^6.1.1",
    "@types/enzyme": "^3.10.3",
    "@types/graphql": "^14.5.0",
    "@types/helmet": "0.0.43",
    "@types/jest": "^24.0.18",
    "@types/locutus": "0.0.5",
    "@types/module-alias": "^2.0.0",
    "@types/node-fetch": "^2.5.2",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^16.9.5",
    "@types/react-dom": "^16.9.1",
    "@types/react-test-renderer": "^16.9.0",
    "@types/storybook__react": "^4.0.2",
    "@types/uuid": "^3.4.5",
    "@zeit/next-bundle-analyzer": "^0.1.2",
    "@zeit/next-sass": "^1.0.1",
    "apollo-server-express": "^2.9.5",
    "body-parser": "^1.19.0",
    "chalk": "^2.4.2",
    "cookie-parser": "^1.4.4",
    "cross-env": "^5.2.1",
    "cssnano": "^4.1.10",
    "debug": "^4.1.1",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "format": "^0.2.2",
    "graphql": "^14.5.8",
    "graphql-cost-analysis": "^1.0.3",
    "graphql-depth-limit": "^1.1.0",
    "helmet": "^3.21.1",
    "locutus": "^2.0.11",
    "module-alias": "^2.2.2",
    "next": "^9.0.8",
    "next-compose-plugins": "^2.2.0",
    "node-sass": "^4.12.0",
    "nprogress": "^0.2.0",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "selfsigned": "^1.10.7",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.3",
    "unhandled-rejection": "^1.0.0",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.8.1",
    "@graphql-codegen/introspection": "^1.8.1",
    "@graphql-codegen/typescript": "^1.8.1",
    "@graphql-codegen/typescript-resolvers": "^1.8.1",
    "@storybook/addon-actions": "^5.2.1",
    "@storybook/addon-links": "^5.2.1",
    "@storybook/addons": "^5.2.1",
    "@storybook/react": "^5.2.1",
    "@typescript-eslint/eslint-plugin": "^2.3.2",
    "@typescript-eslint/parser": "^2.3.2",
    "apollo": "^2.19.0",
    "chokidar": "^3.2.1",
    "concurrently": "^4.1.1",
    "cost-of-modules": "^1.0.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^6.5.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.4.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^1.1.1",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-security": "^1.4.0",
    "http-proxy": "^1.18.0",
    "jest": "^24.9.0",
    "open": "^6.4.0",
    "postcss-loader": "^3.0.0",
    "react-test-renderer": "^16.10.2",
    "rimraf": "^3.0.0",
    "stylelint": "^10.1.0",
    "stylelint-config-standard": "^18.3.0"
  },
  "engines": {
    "node": "10.13.0"
  }
}